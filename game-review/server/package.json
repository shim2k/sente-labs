{
  "name": "aoe4-review-backend",
  "version": "1.0.0",
  "description": "AOE4 Game Review Backend API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "dev:debug": "tsx watch --inspect src/index.ts",
    "start": "node dist/index.js",
    "start:debug": "node --inspect dist/index.js",
    "worker": "tsx src/worker.ts",
    "worker:debug": "tsx --inspect=9230 src/worker.ts",
    "create-db": "tsx src/scripts/create-database.ts",
    "remove-scores": "tsx src/scripts/remove-scores.ts",
    "add-aoe4world-columns": "tsx src/scripts/add-aoe4world-columns.ts",
    "normalize-games": "tsx src/scripts/normalize-games-table.ts",
    "add-winner-columns": "tsx src/scripts/add-winner-columns.ts",
    "update-result-constraint": "tsx src/scripts/update-result-constraint.ts",
    "migrate": "node dist/migrations/migrate.js",
    "logs": "./scripts/view-logs.sh",
    "logs:follow": "./scripts/view-logs.sh -f",
    "logs:error": "./scripts/view-logs.sh error",
    "logs:api": "./scripts/view-logs.sh api-errors",
    "logs:api:follow": "./scripts/view-logs.sh -f api-errors",
    "logs:worker": "./scripts/view-logs.sh worker",
    "logs:worker:follow": "./scripts/view-logs.sh -f worker",
    "logs:worker:error": "./scripts/view-logs.sh worker-error",
    "logs:clear": "rm -f logs/*.log && echo 'All log files cleared'"
  },
  "dependencies": {
    "aws-sdk": "^2.1479.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.0.1",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2"
  }
}
